

<div class="container">
 
 <nav class="navbar ">
  <div class="container-fluid">
    <div class="navbar-header">
      <span class="navbar-brand" style="font-size:46px;">Students</span>
    </div>
    <ul class="nav navbar-nav navbar-right">
              <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#addstudentmodal">Add  New Student  </button>
    <!-- <li id="liuserprofile" class="hidden"><a href="#userprofile">Profile</a></li>  -->
  </ul>
  </div>
</nav>



  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
    <% @students.each do |student|%>
      <tr>
        <td><a href="<%= student_path(student)%>"><%= student.Name%></a></td>
        <td><%= student.email%></td>

        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Delete', student_path(student),
                    method: :delete, data: { confirm: 'Are you sure?' } %></td>
         <!-- <td> <a href="<%= edit_student_path(student)%>"" class="btn btn-info" role="button" data-toggle="modal" data-target="#editstudentmodal">Edit</a></td> -->
      </tr>
      <tr>
      <% end %>
    </tbody>
  </table>
</div>

